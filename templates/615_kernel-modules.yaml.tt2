[%
  hostname = ngcp.get_hostname();

  is_proxy = ngcp.has_role(hostname, 'proxy');
-%]

command:

  lsmod | grep -Eq "^sunrpc ":
    exit-status: 0
    stdout: []
    stderr: []
{{if [% (rtpproxy.enable == "yes" && is_proxy) ? 1 : 0 %] and .Env.NODE_ACTIVE}}

  lsmod | grep -c xt_RTPENGINE:
    exit-status: 0
    stdout:
      - '/^2$/'
    stderr: []
{{end}}
